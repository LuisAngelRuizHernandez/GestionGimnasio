@model ProyectoGimnasio.Maquina

@{
    ViewBag.Title = "Detalles Máquinas";
}

<h2>Detalles de máquina</h2>

<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Nombre de máquina
        </dt>

        <dd>
            @Html.DisplayFor(model => model.nombreMaquina)
        </dd>

        <dt>
            Estado de máquina
        </dt>

        <dd>
            @Html.DisplayFor(model => model.estadoMaquina)
        </dd>

        <dt>
            Nombre Administrador
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Administrador.nombreAdministrador)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Editar", "Edit", new { id = Model.idMaquina }) |
    @Html.ActionLink("Volver", "Index")
</p>
